	public static Node reverseLinkedListInGroups(Node head, int size){
		
		if(head==null || size==1)
			return head;
		
		Node temp=head;
		Node newHead=null;
		Node prevTail=null;
		Node nextHead=null;
		int count=0;
		
		while(temp.next!=null){
			count++;
			if(count==size){
				
			}else if(count%size==0){
				
			}
				
		}
		
		return null;
	}
	
	
	

