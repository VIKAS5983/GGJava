package gg.single;

import gg.single.LinkedListDeletion.Node;

public class LinkedListLength {
	static class Node{
		int data;
 		Node next;
 		Node(int d){
 			data=d;
 			next=null;
 		}
	}
	public static void main(String args[]){
		Node head=new Node(1);
			
	}
	
	
	public static int findLengthIterative(Node head){
		int count=0;
		while(head!=null){
			head=head.next;
			count++;
		}
		return count;
	}
	
	public static int findLengthIterative(Node head){
		int count=0;
		while(head!=null){
			head=head.next;
			count++;
		}
		return count;
	}

}
