package gg.single;

public class LinkedListReverseInGroups {
	public static void main(String args[]){
		Node head=new Node(1);
		head=LinkedListInsertion.insertEnd(head,3);
		head=LinkedListInsertion.insertEnd(head,5);
		head=LinkedListInsertion.insertEnd(head,9);
		head=LinkedListInsertion.insertEnd(head,12);

		Node head1=new Node(2);
		head1=LinkedListInsertion.insertEnd(head1, 6);
		head1=LinkedListInsertion.insertEnd(head1,7);
		head1=LinkedListInsertion.insertEnd(head1,10);
		head1=LinkedListInsertion.insertEnd(head1,15);
		head1=LinkedListInsertion.insertEnd(head1,12);
		head1=LinkedListInsertion.insertEnd(head1,2);
		head1=LinkedListInsertion.insertEnd(head1,8);
		head1=LinkedListInsertion.insertEnd(head1,122);
		head1=LinkedListInsertion.insertEnd(head1,52);
		head1=LinkedListInsertion.insertEnd(head1,6);
		
		printAllNodes(head1);
		sortLinkedLists(head1);
		printAllNodes(head1);

	}
	

}
