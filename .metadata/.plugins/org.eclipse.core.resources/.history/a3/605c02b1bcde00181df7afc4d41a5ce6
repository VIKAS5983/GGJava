package gg.doubly;

public class doubleLinkedListInsertion {
	public static void main(String args[]){
		Node n=new Node(2);
		
		n=insertHead(n,3);
		n=insertHead(n,4);
		n=insertHead(n,5);
		n=insertHead(n,6);
		n=insertHead(n,7);
		n=insertHead(n,8);

		printDoubleLinkedList(n);
	}
	public static void printDoubleLinkedList(Node head){
		while(head!=null){
		System.out.print(" "+head.data);
		head=head.next;
		}
		System.out.println();
	}
	
	public static Node insertHead(Node head,int data){
		Node n=new Node(data);	
		if(head==null){
			return n;
		}
		
		n.next=head;
		head.prev=n;
		n.prev=null;
		return n;
	}
	
	public static Node insertTail(Node head,int data){
		Node n=new Node(data);	
		if(head==null){
			return n;
		}
		Node temp=head;
		while(temp.next!=null){
			
		}
		return null;
	}
	public static Node insertAtIndex(){
		return null;
	}
	
	public static Node insertAfterNode(){
		return null;
	}
	
	public static Node insertBeforeNode(){
		
		return null;
	}
}
