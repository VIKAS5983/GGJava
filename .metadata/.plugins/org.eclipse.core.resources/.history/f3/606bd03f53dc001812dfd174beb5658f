package gg.single;

public class LinkedList {
	
	static class Node{
		int data;
 		Node next;
 		Node(int d){
 			data=d;
 		}
	}
	
	Node head;
	
	
	public static void main(String args[]){
		LinkedList list=new LinkedList();
		list.head=new Node(1);
		
		Node n2=new Node(2);
		Node n3=new Node(3);
		
		
	}
}
