package gg.single;

import gg.single.LinkedList.Node;

public class LinkedListInsertion {

	static class Node{
		int data;
 		Node next;
 		Node(int d){
 			data=d;
 			next=null;
 		}
	}

	public static void main(String args[]){
		Node head=new Node(1);
		
		for(int i=2;i<10;i++){
			head=insertEnd(head,i);
		}
		
		LinkedList.printAllNodes(head)
	}
	
	//Printing all the nodes given the head of the list 
	public static void printAllNodes(Node head){	
		if(list==null)
			return ;
		
		Node tr=list.head;
			while(tr!=null){
				System.out.println("Data "+ tr.data);
				tr=tr.next;
			}

	}

	public static Node insertHead(){
		
		return null;
	}
	
	public static Node  insertEnd(Node head,int value){
		if(head==null)
			return new Node(value);
		Node temp=head;
		Node prev=null;
		while(temp!=null){
			prev=temp;
			temp=temp.next;
		}
		prev.next=new Node(value);
		return head;
	}
	
	public static Node insertInAscendingOrder(){
		
		return null;
	}
	
	public static Node insertAtIndex(){
		
		return null;
	}
	
	public static Node insertInDescendingOrder(){
		
		return null;
	}
}
